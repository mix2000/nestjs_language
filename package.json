        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.0",
        "rimraf": "^5.0.5",
        "rxjs": "^7.8.1",
        "sequelize": "^6.37.0",
